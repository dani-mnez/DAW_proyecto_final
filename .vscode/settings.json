{
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "./styles/css/"
        }
    ],
    "todo-tree.general.tags": [
        "TODO",
        "OJO",
        "FIX",
        "REV",
        "DONE",
        "NOTE"
    ],
    "todo-tree.highlights.customHighlight": {
        "FIX": {
            "icon": "bug",
            "background": "red",
            "fontWeight": "500",
            "foreground": "darkred"
