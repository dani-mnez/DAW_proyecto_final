        "TODO": {
            "icon": "inbox",
            "background": "purple",
            "fontWeight": "500",
            "foreground": "violet"
        },
        "OJO": {
            "icon": "info",
            "background": "yellow",
            "fontWeight": "500",
            "foreground": "black"
        },
        "DONE": {
            "icon": "check",
            "background": "darkgreen",
            "fontWeight": "500",
            "foreground": "limegreen"
        },
        "REV": {
            "icon": "alert",
            "background": "orange",
            "fontWeight": "500",
            "foreground": "#854510"
